<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane onKeyPressed="#keyPressed" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.NewParticipantCtrl">
   <children>
      <TextField fx:id="firstNameField" layoutX="222.0" layoutY="97.0" promptText="First name" onKeyPressed="#keyPressedParticipant"/>
      <TextField fx:id="surNameField" layoutX="222.0" layoutY="133.0" promptText="Surname" onKeyPressed="#keyPressedParticipant"/>
      <TextField fx:id="emailField" layoutX="221.0" layoutY="167.0" promptText="Email" onKeyPressed="#keyPressedParticipant"/>
      <TextField fx:id="ibanField" layoutX="221.0" layoutY="200.0" promptText="IBAN" onKeyPressed="#keyPressedParticipant"/>
      <TextField fx:id="bicField" layoutX="221.0" layoutY="234.0" promptText="BIC" onKeyPressed="#keyPressedParticipant"/>
      <Label layoutX="208.0" layoutY="43.0" text="Add participant">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Button fx:id="addButton" layoutX="281.0" layoutY="292.0" mnemonicParsing="false" onAction="#addParticipant" text="Add" />
      <Label fx:id="errorMessage" alignment="CENTER" layoutX="177.0" layoutY="268.0" prefHeight="12.0" prefWidth="246.0" text="First and Last Name cannot be empty" textFill="RED" visible="false" />
      <Button fx:id="goBackButton" layoutX="534.0" layoutY="30.0" mnemonicParsing="false" onAction="#returnToEvent" text="Return" />
   </children>
</AnchorPane>
